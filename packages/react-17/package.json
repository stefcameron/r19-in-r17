{
  "private": true,
  "name": "@monorepo/react-17",
  "version": "1.0.0",
  "description": "Exploring React 17 base app with React 19 portal and routing between them",
  "engines": {
    "node": ">=21",
    "pnpm": ">=9"
  },
  "scripts": {
    "build": "webpack",
    "build:dev": "NODE_ENV=development webpack",
    "ci:build": "pnpm run build",
    "ci:lint": "pnpm run fmt:check && pnpm run lint",
    "ci:test": "echo 'ğŸ”º No CI tests to run'",
    "fmt": "prettier --write \"{*,src/**/*,tools/**/*,.github/workflows/**/*}.+(js|cjs|mjs|jsx|ts|cts|mts|tsx|css|yml|json)\"",
    "fmt:check": "prettier --check \"{*,src/**/*,tools/**/*,.github/workflows/**/*}.+(js|cjs|mjs|jsx|ts|cts|mts|tsx|css|yml|json)\"",
    "lint": "pnpm run lint:code && pnpm run lint:types && pnpm run fmt:check",
    "lint:code": "eslint \"{*,src/**/*,tools/**/*}.+(js|cjs|mjs|jsx|ts|cts|mts|tsx)\"",
    "lint:types": "tsc --noEmit",
    "start": "NODE_ENV=development webpack-dev-server",
    "test": "pnpm run build && pnpm run lint",
    "verify": "pnpm run fmt && pnpm run lint:code && pnpm run lint:types"
  },
  "author": "Stefan Cameron",
  "license": "MIT",
  "dependencies": {
    "@monorepo/react-19": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/eslint-plugin": "^7.25.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.7",
    "@types/react": "^17.0.83",
    "@types/react-dom": "^17.0.26",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "classnames": "^2.5.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^15.14.0",
    "html-webpack-plugin": "^5.6.3",
    "lodash-es": "^4.17.21",
    "prettier": "^3.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rtvjs": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
